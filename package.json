{
  "name": "alnmouth-golf-crm",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "wrangler pages dev ./dist",
    "deploy": "astro build && wrangler pages deploy ./dist",
    "db:create": "wrangler d1 create alnmouth-golf-db",
    "db:migrate": "wrangler d1 execute alnmouth-golf-db --remote --file=./migrations/001_initial.sql",
    "db:migrate:local": "wrangler d1 execute alnmouth-golf-db --local --file=./migrations/001_initial.sql",
    "db:seed": "wrangler d1 execute alnmouth-golf-db --remote --file=./migrations/002_seed.sql",
    "db:seed:local": "wrangler d1 execute alnmouth-golf-db --local --file=./migrations/002_seed.sql",
    "backup:deploy": "wrangler deploy --config wrangler.backup.toml",
    "backup:logs": "wrangler tail avgc-backup-worker",
    "backup:trigger": "wrangler d1 export alnmouth-golf-db --remote --output avgc-backup.sql"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.0.0",
    "astro": "^5.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241230.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "wrangler": "^4.60.0"
  }
}
